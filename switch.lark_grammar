possible_var : (CNAME ("="NUMBER)* ";")* switch

switch : "switch" "(" var_name ")" "{" (case)* "}"
case :  ("case"|"default") (NUMBER)* ":" CNAME "=" NUMBER ";" "break" ";"
var_name : CNAME


